      <script>
     
  function show_slider(id,img) {
    document.getElementById(id).style.display = "block";
    document.getElementById(img).style.display = "none";
  }
  function hide_slider(id,img) {
    document.getElementById(id).style.display = "none";
    document.getElementById(img).style.display = "block";
  }
  function update(){
    $("#update_address").show();
    $("#shipping_address").hide();
    
}
    $(document).ready(function(){
        $('.carousel').carousel({
            interval: 2000,
            cycle: true,
            pause: "null"
        })
        $("#dashboard_area").show();
        $("#my_store_area").hide();
        $("#designer_dashboard").addClass("menu_item_active");
        $("#my_store").removeClass("menu_item_active");
        $("#designer_dashboard").click(function(){
            $("#dashboard_area").show();
            $("#my_store_area").hide();
            $("#designer_dashboard").addClass("menu_item_active");
            $("#my_store").removeClass("menu_item_active");
            
            
        });
        $("#my_store").click(function(){
            $("#dashboard_area").hide();
            $("#my_store_area").show();
            $("#my_store").addClass("menu_item_active");
            $("#designer_dashboard").removeClass("menu_item_active");
        });
    });
</script>
   </body>
</html>
